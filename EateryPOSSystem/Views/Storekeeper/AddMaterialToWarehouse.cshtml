@model AddMaterialToWarehouseFormModel

@{ 
    ViewData["Title"] = "Add City";
}

<div class="row">
    <div class="col-sm-12 offset-lg-3 col-lg-6 offset-xl-4 col-xl-4">
        <form method="post">
            <h2 class="text-center">Добави град</h2>

            <div class="form-group">
                <label asp-for="Name">Град</label>
                <input asp-for="Name" class="form-control" placeholder="София, Пловдив...">
            </div>
            <div class="form-group">
                <label asp-for="PostalCode">Пощенски код</label>
                <input asp-for="PostalCode" class="form-control" placeholder="1000">
            </div>

            <input class="btn btn-primary" type="submit" value="Save" />
        </form>
    </div>
</div>
