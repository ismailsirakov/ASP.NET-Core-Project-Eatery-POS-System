@model AddMaterialToWarehouseFormModel

@{
    ViewData["Title"] = "Add Material To Warehouse";
}

<div class="row">
    <div class="col-sm-12 offset-lg-2 col-lg-8 offset-xl-2 col-xl-8">
        <form method="post">
            <div class="row">
                <div class="form-group col-4">
                    <label asp-for="DocumentNumber">Номер на документ</label>
                    <input asp-for="DocumentNumber" class="form-control">
                </div>
                <div class="form-group col-4">
                    <label asp-for="DocumentTypeId">Тип документ</label>
                    <select class="form-control">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
                </div>
                <div class="form-group col-4">
                    <label asp-for="DocumentDate">Дата на документ</label>
                    <input asp-for="DocumentDate" class="form-control">
                </div>
            </div>

            <div class="form-group">
                <label asp-for="ProviderId">Доставчик</label>
                <select class="form-control">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="WarehouseId">В склад</label>
                <select class="form-control">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="MaterialId">Материал</label>
                <select class="form-control">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                </select>
            </div>

            <input class="btn btn-primary" type="submit" value="Save" />
        </form>
    </div>
</div>
